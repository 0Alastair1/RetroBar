        <ResourceDictionary Source="Windows 2000.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <BitmapImage x:Key="StartIconImage"
                 UriSource="../Resources/startxpclassic.png" />

    <Style TargetType="MenuItem"
           x:Key="TaskManMenuItem">
